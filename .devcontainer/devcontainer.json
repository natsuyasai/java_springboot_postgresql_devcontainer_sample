{
  "name": "java-api-server",
  "dockerComposeFile": "../docker/docker-compose.yaml",
  // dockercomposeのservice名
  "service": "java",
  // コンテナの作業ディレクトリ
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.windows": "PowerShell"
      },
      "extensions": [
        "vscjava.vscode-java-pack",
        "vmware.vscode-boot-dev-pack"
      ]
    }
  },
}